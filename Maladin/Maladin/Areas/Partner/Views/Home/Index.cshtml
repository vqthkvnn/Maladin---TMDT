
@{
    
    Layout = Request.IsAjaxRequest() ? null : "~/Areas/Partner/Views/Shared/_layout_partner.cshtml";
}
@model Maladin.Areas.Partner.Models.HomeModels
@section Scripts{
    <script>
        @if(Model.iNFOMATION.AVT_ACC==null)
        {
            @:$("p").text("Tài khoản này không có avt");
        }
        else
        {
            @:$('#avt-profile').attr("src",'@Model.iNFOMATION.AVT_ACC');
            @:$('#name-user-profile').text('@Model.iNFOMATION.NAME_INFO');
            @:$("p").text("Tài khoản này không có avt nè");
        }
    
    </script>
}
<div>
    <p></p>
</div>


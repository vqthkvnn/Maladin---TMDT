function getStore(variantid,storPrdTitle,isFirstTime){
	$('#stock-box').html('');
	var storeProvince = {};
	$.ajax({
		url: "/inventory_location.js?variant_id=" + variantid + "&quantity=1",
		success:function(data){
			if( data.inventory_locations.length > 0 ){
				$('#stock-box').show();
				var array_html = '';
				var count_show = 0, hcm_temp = '';
				$.each(data.inventory_locations,function(i,v){
					if(!(storeProvince[v.province_code])){ 
						storeProvince[v.province_code] = v.province_name;
					}
					if(v.location_name.indexOf('Kho ảo online') == -1 && v.location_address.indexOf('Kho trung tâm') == -1 && v.location_address.indexOf('Tô Ngọc Vân') == -1){
						count_show++;
						array_html += "<li data-code='"+v.province_code+"' class='hidden'>";
						array_html += "<span class='bold-light'><img src='https://file.hstatic.net/1000405081/file/maps-and-flags_a7a1277098684b92994772b908989bc6.png' alt='Location'/><span class='name-location-store'>" + v.location_name + " (" + v.location_phone + ")</span></span>";
						array_html += "<span class='desc'>" + v.location_address + "</span></label></li>";
					}
				});
				$('#provinceStore').html('');
				$.each(storeProvince,function(j,k){
					if(j=='HC'){
						hcm_temp = '<option value="'+j+'" data-value="'+k+'">'+k+'</option>';
					}else{
						$('#provinceStore').append('<option value="'+j+'" data-value="'+k+'">'+k+'</option>');
					}
				});
				$('#provinceStore').prepend(hcm_temp);
				$('#provinceStore option:first-child').attr('selected',"selected");
				$('#stock-box').html(array_html);
				$('#stock-box li[data-code="'+$('#provinceStore').val()+'"]').removeClass('hidden');
				$("#add-item-form .viewStore").attr("data-toggle","modal").attr("data-target","#showStore");
			} 
			else {
				$('#stock-box').html('Hiện không còn showroom nào còn sản phẩm '+storPrdTitle);
				$("#add-item-form .viewStore").removeAttr("data-toggle").removeAttr("data-target");
			}
		}
	});
}

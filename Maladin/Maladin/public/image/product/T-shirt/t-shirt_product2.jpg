	function parseQueryString(){
		var str = window.location.search;
		var objURL = {};
		str.replace( new RegExp("([^?=&]+)(=([^&]*))?", "g"),function($0, $1, $2, $3){ objURL[$1] = $3; });
		objURL.count = function(){
			var count = 0;
			for(var prop in this) {
				if(this.hasOwnProperty(prop))
					count = count + 1;
			}
			return count - 1;
		};
		return objURL;
	}
